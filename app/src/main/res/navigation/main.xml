<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    app:startDestination="@id/nav_home">

    <fragment
        android:id="@+id/nav_board"
        android:name="bu.mobile.app.tour.MainActivity"
        android:label="천안의 랜드마크"
        tools:layout="@layout/activity_main" />

    <fragment
        android:id="@+id/nav_weather"
        android:name="bu.mobile.app.tour.WeatherActivity"
        android:label="날씨와 추천관광지"
        tools:layout="@layout/activity_weather" />

    <fragment
        android:id="@+id/nav_home"
        android:name="bu.mobile.app.tour.MypageActivity"
        android:label="마이페이지"
        tools:layout="@layout/activity_mypage" />

    <fragment
        android:id="@+id/nav_quiz"
        android:name="bu.mobile.app.tour.QuizmainActivity"
        android:label="퀴즈풀이"
        tools:layout="@layout/activity_quizmain" />
    <activity
        android:id="@+id/nav_active"
        android:name="bu.mobile.app.tour.QRcodeActivity"
        tools:layout="@layout/activity_q_rcode"
        />

    <!--
    <fragment
        android:id="@+id/nav_settings"
        android:name="com.example.smartfan.SettingsFragment"
        android:label="Settings"
        tools:layout="@layout/fragment_settings" />
    -->
</navigation>